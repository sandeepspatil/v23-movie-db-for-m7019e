<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MovieListFragment">

    <fragment
        android:id="@+id/MovieListFragment"
        android:name="com.ltu.m7019e.v23.themoviedb.MovieListFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_movie_list">

        <action
            android:id="@+id/action_MovieListFragment_to_MovieDetailFragment"
            app:destination="@id/MovieDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/MovieDetailFragment"
        android:name="com.ltu.m7019e.v23.themoviedb.MovieDetailFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_movie_detail">

        <action
            android:id="@+id/action_MovieDetailFragment_to_MovieListFragment"
            app:destination="@id/MovieListFragment" />
        <argument
            android:name="movie"
            app:argType="com.ltu.m7019e.v23.themoviedb.model.Movie" />
        <action
            android:id="@+id/action_MovieDetailFragment_to_three"
            app:destination="@id/three" />
    </fragment>
    <fragment
        android:id="@+id/three"
        android:name="com.ltu.m7019e.v23.themoviedb.Three"
        android:label="fragment_three"
        tools:layout="@layout/fragment_three" >
        <action
            android:id="@+id/action_three_to_MovieListFragment"
            app:destination="@id/MovieListFragment" />
    </fragment>
</navigation>